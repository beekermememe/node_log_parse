assert = require "assert"
Rf = require "#{__dirname}/../src/read_file"

line_to_test = '69.31.28.238 0.003 [14/Nov/2014:00:00:03 -0700] 200 39758 "GET /solr/media_live/select/?q=((tmsid:()+AND+(usid:(9489+184+17533+185+9429+186+9444+187+188+16343))&fq=(start_date_time:[*+TO+2014-11-18T04:00:00Z]+AND+end_date_time:[2014-11-18T00:00:00Z+TO+*])&fl=start_date_time,end_date_time,usid,schedule_id,program_id,program_title,episode_id,episode_theme,is_hd,is_new,egid,episode_description,program_image,callsign,tv_rating,mpaa_rating,channel_image,duration,channel_padded,season_num,episode_num,tms_series_id,mv_tmsid,episode_title,upid&sort=start_date_time+asc&wt=json&rows=1000 HTTP/1.1" "gzip" "gzip" "sg-ipad/4.1.22"'

describe "File Reader", ->
  describe "Extracting stats", ->
    it "extract the number of TMSIDS", ->
      tms_id_count = Rf.breakdownTmsids("tmsid",line_to_test)
      assert.equal(tms_id_count,181)
    it "extract the number of usids", ->
      tms_id_count = Rf.breakdownUsids("usid",line_to_test)
      assert.equal(tms_id_count,10)

  describe "Logging stats", ->
    it "log the number of TMSIDS", ->
      Rf.bucketstat("tmsid",10)
      assert.equal(Rf.stats['tmsid'].under_20, 1)
      Rf.bucketstat("tmsid",10)
      assert.equal(Rf.stats['tmsid'].under_20, 2)
      Rf.bucketstat("tmsid",30)
      assert.equal(Rf.stats['tmsid'].under_100, 1)
      Rf.bucketstat("tmsid",30)
      assert.equal(Rf.stats['tmsid'].under_100, 2)
      Rf.bucketstat("tmsid",120)
      assert.equal(Rf.stats['tmsid'].under_200, 1)
      Rf.bucketstat("tmsid",120)
      assert.equal(Rf.stats['tmsid'].under_200, 2)
      Rf.bucketstat("tmsid",320)
      assert.equal(Rf.stats['tmsid'].over_200, 1)
      Rf.bucketstat("tmsid",320)
      assert.equal(Rf.stats['tmsid'].over_200, 2)    